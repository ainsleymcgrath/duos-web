{
  "name": "duos_research_src",
  "version": "1.0.0",
  "description": "Javascript assets for feedback-collection DUOS application",
  "main": "index.js",
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-preset-env": "^7.0.0-0",
    "babel-preset-react": "^7.0.0-0",
    "bulma": "^0.7.2",
    "concurrently": "^4.0.1",
    "node-sass": "^4.9.4",
    "parcel-bundler": "^1.10.2"
  },
  "scripts": {
    "js-build": "parcel build index.js",
    "js-watch": "parcel watch index.js",
    "css-build": "node-sass \"../sass\" --recursive --output \"../css\" --source-map false",
    "css-watch": "node-sass --watch \"../sass\" --recursive --output \"../css\" --source-map false",
    "start": "concurrently --names \"npm:css-watch\", \"npm:js-watch\"",
    "build": "concurrently --names \"npm:css-build\", \"npm:js-build\""
  },
  "author": "Ainsley McGrath",
  "license": "MIT"
}
